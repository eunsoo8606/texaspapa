{
    "version": 2,
    "rewrites": [
        {
            "source": "/(css|js|images|videos|dist|lib|favicon.ico|robots.txt|sitemap.xml|rss.xml|site.webmanifest|google.*.html|naver.*.html)(.*)",
            "destination": "/public/$1$2"
        },
        {
            "source": "/(.*)",
            "destination": "/app.js"
        }
    ]
}